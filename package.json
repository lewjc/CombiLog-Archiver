  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/node": "16.10.1",
    "@types/request": "^2.48.5",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.2.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
